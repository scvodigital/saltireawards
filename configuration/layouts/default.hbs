  {{#if (contains request.params.uri.pathname '/search/opportunity')}}
    <link rel="canonical" href="https://volunteer.scot{{replace request.params.uri.pathname '/search/opportunity/' '/search/'}}" />
  {{else}}
    <link rel="canonical" href="https://saltireawards.scot{{request.params.uri.pathname}}" />
  {{/if}}

  <link rel="search" type="application/opensearchdescription+xml" title="{{@root.context.metaData.title}}" href="{{~>asset_url url='opensearch.xml'~}}" />
