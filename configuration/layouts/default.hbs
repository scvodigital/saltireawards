  <style>
    @font-face {
      font-family: 'Sonder Sans 3';
      src: url('{{~>asset_url url='fonts/Sonder-Sans3.eot'~}}');
      src: url('{{~>asset_url url='fonts/Sonder-Sans3.eot#iefix'~}}') format('embedded-opentype'),
          url('{{~>asset_url url='fonts/Sonder-Sans3.woff2'~}}') format('woff2'),
          url('{{~>asset_url url='fonts/Sonder-Sans3.woff'~}}') format('woff'),
          url('{{~>asset_url url='fonts/Sonder-Sans3.eot'~}}') format('truetype'),
          url('{{~>asset_url url='fonts/Sonder-Sans3.svg#Sonder-Sans3'~}}') format('svg');
      font-weight: normal;
      font-style: normal;
    }
