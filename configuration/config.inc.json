{
  "domains": [
    "saltireawards.local",
    "test.saltireawards.scot",
    "saltireawards.scot",
    "saltireawards.org.uk"
  ],
  "routes": [
    "{:./routes/*.json}"
  ],
  "metaData": {
    "assetsVersion": "{:../package.json@version}",
    "assetsBase": "https://storage.googleapis.com/scvo-assets/test/saltireawards/",
    "title": "Saltire Awards",
    "fullTitle": "The Saltire Awards",
    "tagline": "Celebrating youth volunteering in Scotland",
    "certificateName": "Saltire Award",
    "accountName": "My Saltire",
    "url": "https://saltireawards.scot",
    "twitter": "saltireawards",
    "contacts": {
      "admin": "support@saltireawards.scot",
      "noreply": "no-reply@saltireawards.scot",
      "technical": "webhelpdesk@scvo.org.uk"
    },
    "youngScotSettings": "{:./data/young-scot-settings.json}",
    "emailSendSettings": "{:./data/email-settings.json}",
    "theme": {
      "primary": "#008EDF",
      "secondary": "#1B2875",
      "challenge": "#008DDE",
      "approach": "#3BAA34",
      "ascent": "#F05B23",
      "summit": "#514680",
      "footer": "#591669",
      "copyright": "#FEF9F9"
    },
    "elasticsearchUrl": "{$./data/elasticsearch-url.txt}",
    "menus": "{:./data/menu-layout.json}",
    "certificates": "{:./data/info-certificates.json}",
    "tsiList": "{:./data/info-volunteering-organisations.json}",
    "taskInfoValidation": "{:./data/validation-tasks.json}",
    "accountInfoValidation": "{:./data/validation-account.json}",
    "globalGoals": "{:./data/info-global-goals.json}",
    "volunteeringTerms": "{:./data/info-volunteering-terms.json}",
    "handlebars": {
      "partials": {
        "{$./partials/**/*.*}": "includeRelativePath=false&regexExpression=(-)&regexOptions=gi&regexReplace=_"
      },
      "templates": {
        "{$./templates/**/*.*}": ""
      },
      "layouts": {
        "{$./layouts/**/*.*}": ""
      },
      "queries": {
        "{$./queries/**/*.*}": ""
      },
      "frontend": {
        "tasks": {
          "{$./frontend/tasks/**/*.*}": ""
        },
        "configs": {
          "{$./frontend/configs/**/*.*}": ""
        },
        "partials": {
          "{$./frontend/partials/**/*.*}": "includeRelativePath=false&regexExpression=(-)&regexOptions=gi&regexReplace=_"
        }
      }
    },
    "commonTasks": "{:./data/common-tasks.json}"
  }
}